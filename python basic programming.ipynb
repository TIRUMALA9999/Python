   "metadata": {},
   "outputs": [],
   "source": [
    "def is_armstrong(n):\n",
    "    temp=n\n",
    "    t=len(str(n))\n",
    "    sum=0\n",
    "    while(temp>0):\n",
    "        n=temp%10\n",
    "        sum+=pow(n,t)\n",
    "        temp=temp//10\n",
    "    return sum"
   ]
  },
